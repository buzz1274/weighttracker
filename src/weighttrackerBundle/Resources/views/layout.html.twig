<!DOCTYPE html>
<html>
<head>
    <meta http-equiv="Content-Type" content="text/html; charset={{ _charset }}"/>
    <title>{% block title %}{% endblock %}</title>
    <link rel="icon" sizes="16x16" href="{{ asset('bundles/weighttracker/images/favicon.ico') }}" />
    <link href="//netdna.bootstrapcdn.com/bootstrap/3.1.1/css/bootstrap.min.css" rel="stylesheet" type="text/css" media="all" />
    <link href="{{ asset('bundles/weighttracker/css/bootstrap-responsive.css') }}" rel="stylesheet" type="text/css" media="all" />
    <link href="{{ asset('bundles/weighttracker/css/styles.css') }}" rel="stylesheet" type="text/css" media="all" />
    {% block head %}{% endblock %}
</head>
<body>
    {% block body %}{% endblock %}
    {% block javascripts %}
        {% javascripts
            '@weighttrackerBundle/Resources/public/js/libs/jquery-1.10.2.js'
            '@weighttrackerBundle/Resources/public/js/libs/handlebars-1.1.2.js'
            '@weighttrackerBundle/Resources/public/js/libs/ember-1.5.1.js'
            '//netdna.bootstrapcdn.com/bootstrap/3.1.1/js/bootstrap.min.js'
            '@weighttrackerBundle/Resources/public/js/app.js'
            filter='uglifyjs2'
        %}
            <script type="text/javascript" src="{{asset_url}}"></script>
        {% endjavascripts %}
    {% endblock %}
</body>
</html>